#include <stdio.h>

int main() {
    int a[100], i, n, j, temp;
    
    printf("Enter Number Of Elements : ");
    scanf("%d", &n);
    
    printf("Enter Elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - i - 1; j++)
        {
            if (a[j] > a[j + 1]) 
            {   
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
        }
    }

    printf("Sorted Array:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);  
    }

    return 0;
}


OUTPUT:
Enter Number Of Elements : 4
Enter Elements:
2
5
7
8
Sorted Array:
2 5 7 8 
