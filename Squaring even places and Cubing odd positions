#include <stdio.h>

int main() {
    int arr[100], i, n;

    printf("Enter Number Of Elements: ");
    scanf("%d", &n);

    printf("Enter Elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for (i = 0; i < n; i++) {
        if ((i + 1) % 2 == 0) { 
            arr[i] = arr[i] * arr[i];
        }else {
            arr[i]=arr[i]*arr[i]*arr[i];
        }
    }

    printf("New Array:\n");
    for (i = 0; i < n; i++) {
        printf("%d\n", arr[i]);
    }

    return 0;
}

OUTPUT:
Enter Number Of Elements: 4
Enter Elements:
2
5
4
3
New Array:
8
25
64
9

